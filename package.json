{
	"name": "@wx/svelte-core",
	"version": "1.2.3",
	"productTag": "core",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src --ext .svelte,.ts,.js",
		"start": "vite --open=/demos/",
		"start:tests": "vite --open=/tests/ --mode test"
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"dependencies": {
		"@wx/core-locales": "1.2.3",
		"@wx/lib-dom": "0.5.3"
	},
	"files": [
		"src",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	]
}
